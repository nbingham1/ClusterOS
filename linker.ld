ENTRY(loader)

SECTIONS
{
    . = 0x00100000;
	_start = .;
	PROVIDE(start = .);
	
    .text : {
        *(.text)
    }

    .rodata ALIGN (0x1000) : {
        *(.rodata)
    }

    .data ALIGN (0x1000) : {
        start_ctors = .;
        *(.ctor*)
        end_ctors = .;
        start_dtors = .;
        *(.dtor*)
        end_dtors = .;
        *(.data)
    }

    .bss : {
        _sbss = .;
        *(COMMON)
        *(.bss)
        _ebss = .;
    }
    
    _end = .;
    PROVIDE(end = .);
}
